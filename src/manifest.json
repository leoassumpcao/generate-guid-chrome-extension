{
    "name": "Populate Input with GUID",
    "description": "Populates an input with a random generated GUID.",
    "version": "0.1.0",
    "manifest_version": 3,
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "all_frames": true,
        "match_about_blank": true
    }],
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "contextMenus"
    ],
    "host_permissions": [
        "<all_urls>"
    ]
}